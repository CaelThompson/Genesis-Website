---
import BaseLayout from "../layouts/SearchLayout.astro";
import News1 from "../components/landing/NewsPage/News1.astro";
import NewsLink from "../components/landing/NewsPage/NewsLink.astro";
import { articles } from "../components/landing/NewsPage/News1Storage.js";

---
<BaseLayout>
    <News1/>
    {articles.map((article) => (
        <NewsLink 
          title={article.title} 
          description={article.description} 
          image={article.image} 
          url={`/news1/${article.slug}`},
          date={article.date}
        />
      ))}
</BaseLayout>

